Private Sub Form_Unload(Cancel As Integer)
   If vgMaster.Busy Then    'if the component is doing a task, exiting immediately could cause access violations
      Cancel = True         'so we cancel the upload, we will do it when the OnLastTaskCompleted event occurs
      RequestUnload = True
   End If
End Sub

Private Sub vgMaster_OnLastCommandCompleted()
   If RequestUnload Then
      Unload Form1          'now we can unload the form
   End If
End Sub

Private Sub Form_Load()
    RequestUnload = False
   
    AssignListToComboBox cboVideoDevices, vgMaster.VideoDevices, -1
    
    If vgMaster.VideoDevice = -1 Then
        cboVideoDevices.Text = "SELECT FIRST A VIDEO CAPTURE DEVICE IN THIS LIST"
    End If
    
    vgMaster.MultiplexedRole = mr_MultiplexedMaster
    vgSlave1.MultiplexedRole = mr_MultiplexedSlave
    vgSlave2.MultiplexedRole = mr_MultiplexedSlave
    vgSlave3.MultiplexedRole = mr_MultiplexedSlave
    vgSlave4.MultiplexedRole = mr_MultiplexedSlave
    
    Call vgMaster.AssociateMultiplexedSlave(0, vgSlave1.UniqueID)
    Call vgMaster.AssociateMultiplexedSlave(1, vgSlave2.UniqueID)
    Call vgMaster.AssociateMultiplexedSlave(2, vgSlave3.UniqueID)
    Call vgMaster.AssociateMultiplexedSlave(3, vgSlave4.UniqueID)
    
    vgMaster.Display_AutoSize = False
    vgSlave1.Display_AutoSize = False
    vgSlave2.Display_AutoSize = False
    vgSlave3.Display_AutoSize = False
    vgSlave4.Display_AutoSize = False

    'vgMaster.AnalogVideoStandard = vgMaster.FindIndexInListByName(vgMaster.AnalogVideoStandards, "PAL", True, True)
    
    vgMaster.MultiplexedInputEmulation = False
    vgSlave1.MultiplexedInputEmulation = False
    vgSlave2.MultiplexedInputEmulation = False
    vgSlave3.MultiplexedInputEmulation = False
    vgSlave4.MultiplexedInputEmulation = False
    
End Sub

